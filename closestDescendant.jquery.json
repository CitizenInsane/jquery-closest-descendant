    },
    "bugs": "https://github.com/tlindig/jquery-closest-descendant/issues",
    "demo": "http://tlindig.github.com/jquery-closest-descendant/",
    "download": "https://github.com/tlindig/jquery-closest-descendant/blob/master/dist/closestDescendant.min.js",
    "licenses": [{
